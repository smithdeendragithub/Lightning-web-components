<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Approval_Interval__c</fullName>
    <description>Calculated number of days between submitted for approval and approval dates</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISNULL(Submitted_On__c)), Approved_On__c - Submitted_On__c, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculated number of days between submitted for approval and approval dates</inlineHelpText>
    <label>Approval Interval</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
