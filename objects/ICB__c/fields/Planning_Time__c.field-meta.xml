<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Planning_Time__c</fullName>
    <description>Calculated number of days between created and submitted dates</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISNULL(Submitted_On__c)), Submitted_On__c -  CreatedDate, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculated number of days between created and submitted dates</inlineHelpText>
    <label>Planning Time</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
