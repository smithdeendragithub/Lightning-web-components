<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CANNOT_CHANGE_ATTRIBUTE</fullName>
    <active>false</active>
    <description>User cannot change an attribute on a workstep.</description>
    <errorConditionFormula>ISCHANGED(Attribute__c)</errorConditionFormula>
    <errorDisplayField>Attribute__c</errorDisplayField>
    <errorMessage>Attribute should not be modified.</errorMessage>
</ValidationRule>
