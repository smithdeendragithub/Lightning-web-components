<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Turndown__c</fullName>
    <description>Copy value from MRC__c if the record type contains &#39;Turndown&#39;. Added this field for Pike&#39;s report</description>
    <externalId>false</externalId>
    <formula>IF(CONTAINS(RecordType.Name, &#39;Turndown&#39;),  MRC__c, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Turndown</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
