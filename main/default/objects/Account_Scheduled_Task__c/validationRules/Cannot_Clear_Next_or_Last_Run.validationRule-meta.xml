<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_Clear_Next_or_Last_Run</fullName>
    <active>true</active>
    <errorConditionFormula>(ISCHANGED(Next_Run__c) &amp;&amp; ISNULL(Next_Run__c)) || (ISCHANGED(Last_Run__c) &amp;&amp; ISNULL(Last_Run__c))</errorConditionFormula>
    <errorMessage>The Next Run and Last Run dates cannot be cleared</errorMessage>
</ValidationRule>
